"FrontEndDNSName": {
  "Type": "AWS::Route53::RecordSet",
  "Properties": {
    "HostedZoneName": {
      "Fn::Join": ["", [{
        "Ref": "HostedZone"
      }, "."]]
    },
    "Comment": "CNAME redirect custom name to CloudFront distribution",
    "Name": {
      "Fn::Join": ["", [{
        "Ref": "AWS::StackName"
      }, {
        "Ref": "AWS::AccountId"
      }, ".", {
        "Ref": "AWS::Region"
      }, ".", {
        "Ref": "HostedZone"
      }]]
    },
    "Type": "CNAME",
    "TTL": "900",
    "ResourceRecords": [{
      "Fn::Join": ["", ["http://", {
        "Fn::GetAtt": ["FrontEndCDN", "DomainName"]
      }]]
    }]
  }
}

"Outputs": {
  "FrontEndURL": {
    "Value": {
      "Fn::Join": ["", ["http://", {
        "Ref": "FrontEndDNSName"
      }]]
    },
    "Description": "The URL of the newly created website"
  }
}